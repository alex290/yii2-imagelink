{
    "name": "alex290/yii2-imagelink",
    "description": "You can get image URLs from image page HTML.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","imagelink"],
    "license": "MIT",
    "authors": [
        {
            "name": "WebGoal",
            "email": "alexmar12@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "symfony/yaml": "*",
        "pear/http_request2": "*",
        "sunra/php-simple-html-dom-parser": "v1.5.0"
    },
    
    "autoload": {
        "psr-4": {
            "alex290\\imagelink\\": ""
        }
    },
    "include-path": [
        "vendor/pear/net_url2",
        "vendor/pear/http_request2/HTTP"
    ]
}
